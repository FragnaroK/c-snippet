{
  "name": "c-snippet",
  "version": "0.0.1",
  "description": "Convert snippets from any code editor",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json" 
  },
  "author": "FragnaroK",
  "license": "MIT",
  "_moduleAliases": {
    "@root": "/src",
    "@interfaces": "/src/types/types.ts",
    "@utils": "/src/utils"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/module-alias": "^2.0.1",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "cson-parser": "^4.0.9",
    "eslint": "^8.46.0",
    "module-alias": "^2.2.3"
  }
}
