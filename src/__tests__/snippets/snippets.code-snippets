	"Carousel basic of TQ framework Js": {
	  "prefix": "carousel-basic-js",
	  "body": [
		"$(function() {",
		"",
		"    $('.slider-extra-content-v2').carousel({",
		"        interval: false,",
		"        wrap: false",
		"    })",
		"    $('.slider-extra-content-v2').find(\".carousel-control.left\").hide()",
		"    $('.slider-extra-content-v2').find(\".carousel-caption\").hide()",
		"    $('.slider-extra-content-v2').each(function(i) {",
		"        $(this).find(\".carousel-control.left\").attr(\"aria-labelledby\", 'scr-rdr-' + i + '')",
		"        $(this).find(\".carousel-control.right\").attr(\"aria-labelledby\", 'scr-rdr-' + i + '')",
		"        $('<div id=\"scr-rdr-' + i + '\" aria-live=\"polite\" class=\"slider-extra-content-container\"></div>').insertAfter($(this))",
		"        insertText($(this), 0)",
		"    })",
		"    $('.slider-extra-content-v2').on('slide.bs.carousel', function(e) {",
		"        if (e.direction == \"left\") {",
		"            if ($(e.relatedTarget).index() == $(this).find(\".carousel-indicators\").children().last().index()) {",
		"                $(this).find(\".carousel-control.right\").fadeOut()",
		"                $(this).find(\".carousel-control.left\").fadeIn().focus()",
		"            } else {",
		"                $(this).find(\".carousel-control\").fadeIn()",
		"            }",
		"        } else if (e.direction == \"right\") {",
		"            if ($(e.relatedTarget).index() == 0) {",
		"                $(this).find(\".carousel-control.left\").fadeOut()",
		"                $(this).find(\".carousel-control.right\").fadeIn().focus()",
		"            } else {",
		"                $(this).find(\".carousel-control\").fadeIn()",
		"            }",
		"        }",
		"        insertText($(this), $(e.relatedTarget).index())",
		"    })",
		"",
		"    function insertText(ctx, i) {",
		"        var html = ctx.find(\".item\").eq(i).find(\".carousel-caption\").html()",
		"        ctx.next(\".slider-extra-content-container\").html(html)",
		"    }",
		"})",
		"",
		"    function addSlides(slides) {",
		"        slides.forEach((slide, index) => { ",
		"",
		"            document.getElementById(\"carousel-indicators-list\").innerHTML += `",
		"                <li data-target=\"#carousel-fb2a2cd8-667d-4945-9340-16df85a94ee2\" data-slide-to=\"\\${index}\" ${index == 0? \"class=\\\"active\\\"\" : \"\"}></li>",
		"            `",
		"            ",
		"            document.getElementById(\"slidesCont\").innerHTML += `",
		"                <div class=\"item ${ index == 0? \"active\" : \"\" }\" role=\"option\">",
		"                    <img src=\"${slide.image}\" class=\"ca_image\" alt=\"${slide.alt}\">",
		"                    <div class=\"carousel-caption\">",
		"                        <div class=\"top-buffer\"></div>",
		"                        ${slide.content}",
		"                        <p class='small text-muted text-right'>${slide.copy}</p> ",
		"                    </div>",
		"                </div>",
		"                ` ",
		"        })",
		"      $(\".carousel-tablist-highlight\").remove() // DO NOT DELETE THIS LINE, cuz it's the reason of the horizontal overflow. Temporal solution.",
		"    }",
		"",
		"    addSlides([",
		"    {",
		"        image: \"{{img_path}}/XXXXXXXXX.jpeg\",",
		"        alt: \"XXXXXX\",",
		"        copy: \"XXXXXX / WWWWWWW / UUUUUUU.com\",",
		"        content: \"XXXXXXXXXXXXXXX\"",
		"    }",
		"    ])"
	  ],
	  "description": "Carousel basic of TQ framework"
	},
