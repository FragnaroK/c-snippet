{
  "name": "c-snippet",
  "version": "0.0.1",
  "description": "Convert snippets from any code editor",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "build:tsc": "tsc",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "repo:commit": "git add . && git commit && git push origin"
  },
  "author": "FragnaroK",
  "license": "MIT",
  "_moduleAliases": {
    "@root": "/src",
    "@interfaces": "/src/types/types.ts",
    "@utils": "/src/utils"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/module-alias": "^2.0.1",
    "@types/xml2js": "^0.4.11",
    "clean-webpack-plugin": "^4.0.0",
    "jest": "^29.6.2",
    "jest-ado-reporter": "^0.0.3",
    "jest-simple-dot-reporter": "^1.0.5",
    "prettier": "^3.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "cson-parser": "^4.0.9",
    "eslint": "^8.46.0",
    "fast-xml-parser": "^4.2.7",
    "htmlparser2": "^9.0.0",
    "module-alias": "^2.2.3",
    "path-browserify": "^1.0.1",
    "prompt-sync": "^4.2.0",
    "xml-parse": "^0.4.0",
    "xml2js": "^0.6.2",
    "yargs": "^17.7.2"
  }
}
