	  "Resize of TQ framework - JS": {
		"prefix": "tq-bespoke-resize-js",
		"body": [
		  "function bespokeResize(where, data) {",
		  "        let bespokeResize = document.getElementById(where);",
		  "        bespokeResize.ariaHidden = true;",
		  "        bespokeResize.innerHTML += `",
		  "        <div class='\\${where}-text-box'>",
		  "            <p>${data.instructions}</p>",
		  "        </div>",
		  "        `",
		  "        data.box.forEach((button) => {",
		  "            if (button == data.box[0]) button.styles = \"border-bottom-left-radius: 5px; border-top-left-radius: 5px;\"",
		  "            if (button == data.box[data.box.length - 1]) button.styles = \"border-bottom-right-radius: 5px; border-top-right-radius: 5px;\"",
		  "            bespokeResize.innerHTML += `",
		  "            <div tabindex=\"0\" data-color=\"${button.bgColor}\" data-text=\"${button.content}\" class=\"\\${where}-box ${button.class? button.class : \"\"}\" style=\"outline: none !important; background-color: ${button.bgColor} !important; ${button.styles? button.styles : \"\"}\">${button.name}</div>",
		  "            `",
		  "            ",
		  "        });",
		  "        ",
		  "        $(`.\\${where}-box`).click(function() {",
		  "            changeBespoke1($(this))",
		  "          })",
		  "          ",
		  "          $(`.\\${where}-box`).on(\"keydown\", function(e) {",
		  "            if (e.which == 13)",
		  "              changeBespoke1($(this))",
		  "          })",
		  "          ",
		  "          function changeBespoke1(ctx) {",
		  "            $(`.\\${where}-text-box`).slideUp(\"fast\", function() {",
		  "              $(`.\\${where}-text-box`).html(ctx.data(\"text\"))",
		  "              $(`.\\${where}-text-box`).css(\"backgroundColor\", ctx.data(\"color\"))",
		  "              $(`.\\${where}-text-box`).slideDown()",
		  "            })",
		  "          }",
		  "    }",
		  "",
		  "    bespokeResize(\"${1:name}\", {",
		  "        instructions: \"Select each button below to learn more.\",",
		  "        box: [",
		  "        {",
		  "            name: \"1\",",
		  "            content: \"text\",",
		  "            bgColor: \"#FF8037\"",
		  "        },",
		  "        {",
		  "            name: \"2\",",
		  "            content: \"text\",",
		  "            bgColor: \"#F5D619\"",
		  "        },",
		  "        {",
		  "            name: \"3\",",
		  "            content: \"text\",",
		  "            bgColor: \"#17B9E8\"",
		  "        }]",
		  "    })",
		  "    "
		],
		"description": "Resize of TQ framework"
	  },
