{
  "name": "c-snippet",
  "version": "0.0.1",
  "description": "Convert snippets from any code editor (TS)",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "cls && node lib/index.js",
    "build": "cls && npx tsc",
    "build:watch": "cls && npx tsc && node lib/index.js",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "repo:commit": "git add . && git commit && git push origin"
  },
  "author": "FragnaroK",
  "license": "MIT",
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/jest": "^29.5.3",
    "@types/module-alias": "^2.0.1",
    "@types/xml2js": "^0.4.11",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.0.1",
    "chalk": "^5.3.0",
    "cson-parser": "^4.0.9",
    "eslint": "^8.46.0",
    "figlet": "^1.6.0",
    "htmlparser2": "^9.0.0",
    "xml2js": "^0.6.2"
  }
}
